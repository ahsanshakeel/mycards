@model IEnumerable<MyCard.Web.Models.InquiryListViewModel>
@{
    Layout = null;
}
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>LeadsExport</title>
</head>
<body>
    <div>
        <table class="table table-striped table-bordered table-condensed">
            <tr>
                <th>Business Name</th>
                <th>Customer Name</th>
                <th>Email</th>
                <th>Inquiry Date</th>
            </tr>
            @foreach (MyCard.Web.Models.InquiryListViewModel item in Model)
            {
                <tr>
                    <td>@item.BusinessName</td>
                    <td>@item.CustomerName</td>
                    <td>@item.Email</td>
                    <td>@item.InquiryDate</td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
