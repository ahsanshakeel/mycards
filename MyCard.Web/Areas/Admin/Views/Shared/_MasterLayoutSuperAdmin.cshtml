<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title		|		My Cards</title>

    <!-- Bootstrap -->

    <link href="~/css/bootstrap.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" />
    <link href="~/css/admin.css" rel="stylesheet" />
    <link href="~/Content/themes/base/jquery-ui.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/font-awesome.min.css">

    <script src="~/Scripts/jquery-3.5.1.min.js"></script>
    @*<script src="~/Scripts/jquery-1.12.4.min.js"></script>*@
    <script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/Scripts/Chart.min.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>



    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
          <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
</head>
<body style="background:#FFFFFF;">

    <div class="col-xs-3 pull-left l-panel">
        <div class="row">
            <div class="col-xs-offset-1 col-xs-6 mar-top20 mar-bot40">
                <a href="#"><img alt="logo" class=" img-responsive" src="~/img/admin/register-logo.png" /></a>
            </div>
            <br>
            <br>

            <div class="col-xs-12">
                <div class="mar-top40">

                    <h3 class="thr color-white text-center mar-top40">
                        Welcome to your<br>
                        administration portal!
                    </h3>


                    <div class=" col-xs-offset-1 col-xs-10 mar-top20">
                        <img alt="" class="img-responsive center-block mar-top40" src="~/img/admin/left-img.png" />

                    </div>


                </div>

            </div>
        </div>
    </div>

    <div class="col-xs-9 pull-right ">
        <div class="col-xs-12">
            <div class="m-both">
                <nav class="nav-tabs mar-left20 mar-right20">
                    <div class="row">
                        <div class="">
                            <!-- Brand and toggle get grouped for better mobile display -->
                            <div class="navbar-header">
                                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1"> <span class="sr-only">Toggle navigation</span> <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> </button>
                            </div>

                            <!-- Collect the nav links, forms, and other content for toggling -->
                            <div class="">
                                <div class="collapse navbar-collapse mar-top20" id="bs-example-navbar-collapse-1">
                                    <ul class="nav navbar-nav navbar-left">
                                        <li><a class="active nav-item f-p" href="@Url.Action("Index", "Default")">Admin Dashboard</a> </li>
                                        @*<li><a class="nav-item f-p" href="@Url.Action("Purchase", "CompanyDashboard")">Purchase</a> </li>
                                            <li><a class="nav-item f-p" href="#">myCards Designer</a> </li>
                                            <li><a class="nav-item f-p" href="@Url.Action("Index", "CompanyDashboard")">myProfile</a> </li>*@
                                    </ul>
                                    <div class="navbar-form navbar-right">
                                        @if (Request.IsAuthenticated)
                                        {
                                            <a href="@Url.Action("Logout", "Default")" class="btn btn-default signin mar-top2 medium f-p">Sign Out</a>
                                        }

                                    </div>
                                    @*<div class="navbar-right mar-top15 mar-right10 book f-p">myCards™ Home</div>*@
                                </div>
                            </div>
                            <!-- /.navbar-collapse -->
                        </div>
                        <!-- /.container-fluid -->
                    </div>
                </nav>
            </div>
        </div>
        <div class=" col-xs-12 mar-top40">

            <div id="divRegForm" class="signin-form ">

                @RenderBody()

            </div>
        </div>
        <div class="clearfix"></div>
        <div class=" sa-footer">

            <div class="pull-left col-xs-6">
                <div class="row">
                    <p class="p-small">
                        <a class="mar-right10 dark-color p-small" href="#">Terms & conditions</a> • <a class="mar-right10 dark-color p-small" href="#"> &nbsp; Privacy policy</a>
                    </p>
                </div>
            </div>
            <div class="pull-right col-xs-6">
                <div class="row">
                    <p class="text-right dark-color p-small mar-bot0">© <span class="medium">myCards™</span> All Rights Reserved. 2020</p>
                </div>
            </div>
        </div>

    </div>

</body>
</html>

@*<script src="~/Scripts/jquery-1.8.0.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>*@

