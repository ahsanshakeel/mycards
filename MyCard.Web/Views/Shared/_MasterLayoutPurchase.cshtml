<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title		|		My Cards</title>

    <!-- Bootstrap -->
    <link href="~/css/bootstrap.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" />
    <link href="~/css/admin.css" rel="stylesheet" />
    <link href="~/Content/themes/base/jquery-ui.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/font-awesome.min.css">

    <script src="~/Scripts/jquery-3.5.1.min.js"></script>
    @*<script src="~/Scripts/jquery-1.12.4.min.js"></script>*@
    <script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>



    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
          <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
</head>
<body style="background:#FFFFFF;">


    <div class="col-xs-4 pull-left l-panel">
        <div class="row">
            <div class="col-xs-offset-1 col-xs-5 mar-top20 mar-bot40">
                <a href="@System.Configuration.ConfigurationManager.AppSettings["myCardWeb"]"><img alt="logo" class="img-responsive" src="~/img/admin/register-logo.png" /></a>
            </div>
            <br>
            <br>
            <div class="col-xs-12">
                <div class="mar-top30">
                    <div class=" col-xs-offset-2 col-xs-8 mar-top40">
                        @if (ViewBag.PageType == "PurchaseUpload")
                        {
                            <img alt="" class="img-responsive center-block mar-top40" src="~/img/admin/left-img-purchase2.png" />
                        }
                        else
                        {
                            <img alt="" class="img-responsive center-block mar-top40" src="~/img/admin/left-img-purchase.png" />
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xs-8 pull-right well-btm">
        <div class="col-xs-12">
            <div class="m-both">
                @Html.Partial("_Navbar")
            </div>
        </div>
        <div class="col-lg-12">
            <div class="pull-right">
                <div class="usd">
                    <p><img src="~/img/admin/c-usa.png">  US Dollars </p>
                </div>
            </div>
        </div>

        @RenderBody()

        <div class="clearfix"></div>
        @Html.Partial("_Footer")
    </div>
    <div class="clearfix"></div>


</body>
</html>


<script src="~/Scripts/main.js"></script>